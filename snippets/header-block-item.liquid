{% liquid 
  if settings.lang_direction contains request.locale.iso_code
    assign is_rtl = true
    assign class_image_ratio = "w-8 h-8 ml-4"
  else
    assign is_rtl = false
    assign class_image_ratio = "w-8 h-8 mr-4"
  endif
  assign text_base_size =  settings.text_base_size | times: 0.01
  assign menu_font_weight = settings.type_menu_font.weight | plus: 0
%}
{%- case type_menu -%}
  {%- when 'tabbed_menu' -%}
    {%- liquid
      assign image_count = 0
      if block.settings.image_1 != blank
        assign image_count = image_count | plus: 1
      endif
      if block.settings.image_2 != blank
        assign image_count = image_count | plus: 1
      endif
      if block.settings.image_3 != blank
        assign image_count = image_count | plus: 1
      endif
      assign quick_access_count = 0
      if block.settings.enable_quick_access_blocks
        if block.settings.block_title != blank
          assign quick_access_count = 1
        endif
        if block.settings.block_title_2 != blank
          assign quick_access_count = 1
        endif
        if block.settings.block_title_3 != blank
          assign quick_access_count = 1
        endif
        if block.settings.block_title_4 != blank
          assign quick_access_count = 1
        endif
      endif 
      assign image_count = image_count | plus: quick_access_count
      assign total_block = image_count | minus: quick_access_count
      assign class_menu_horizontal = 'grid-cols-4'
      if image_count > 0
        assign class_image_width = 'w-1/5'
        if total_block > 0 and quick_access_count > 0
          assign class_menu = 'md:w-3/5 grid-cols-3'
        else 
          assign class_menu = 'w-3/4 grid-cols-3'
        endif
        if block.settings.promotion_layout == 'horizontal'
          assign class_image_width = 'w-1/5'
          if total_block > 0 and quick_access_count > 0
            assign class_menu = 'w-3/5 grid-cols-3'
          endif
          if total_block == 2
            assign class_image_width = 'w-1/2'
            assign class_menu = 'w-1/2 2xl:w-5/12 grid-cols-2'
            assign class_menu_horizontal = 'grid-cols-3'
          endif
          if total_block >= 3
            assign class_image_width = 'w-7/12'
            unless block.settings.tab_style == "vertical" and block.settings.enable_quick_access_blocks == true
              assign class_menu_horizontal = 'grid-cols-2'
              assign class_menu = 'w-5/12 2xl:w-[38.75%] grid-cols-2'
            else
              assign class_menu_horizontal = 'grid-cols-1'
              assign class_menu = 'w-5/12 2xl:w-[38.75%] grid-cols-1'
            endunless
          endif
        endif
      else
        assign class_menu = 'grid-cols-4'
      endif
    -%}
    {%- if block.settings.menu.links or image_count > 0 -%}
      <template x-if="!runDispatch">
        <div class="toggle-menu toggle-menu-hidden background-header{% if quick_access_count == 0 or total_block == 0 %} mega-menu-{{ block.settings.tab_style }}{% endif %}{% if image_count > 0 and  quick_access_count <= 0 %} main-nav-child-{{ block.settings.promotion_position }}{% endif %} is-focus:text-[rgba(var(--colors-text))] text-[rgba(var(--colors-text))] absolute w-full background-header left-0 z-50 shadow-md ltr">
          <div class="mega-menu-container x-pgw page-width mx-auto promotion-{{ block.settings.promotion_layout }} image-count-{{ image_count }}{% if block.settings.tab_style == 'vertical' %} md:max-h-[80vh]{% endif %}{% if quick_access_count > 0 %} flex gap-5 xl:gap-8{% endif %}">
            <div class="mega-nav pt-8 relative{% if block.settings.tab_style == 'vertical' %}{% if is_rtl %} rtl{% endif %}{% else %} pb-8{% endif %} {{ class_menu }} relative{% if quick_access_count > 0 %} order-2{% endif %} mega-menu-{{ block.settings.tab_style }}">
              {% if block.settings.tab_style == 'vertical' %}
                <ul class="mega-list-nav flex-wrap items-end{% if block.settings.tab_style == 'vertical' %} w-72 md:max-h-[calc(80vh-3rem)] md:overflow-y-scroll scrollbar-body scrollbar-visible{% endif %}{% if is_rtl %} rtl justify-start{% endif %}" x-refs="mega_nav"> 
                  {%- for link in block.settings.menu.links -%}
                    <li
                      class="sub-menu-item table{% if link.links != blank %} has-dropdown{% endif %}{% if block.settings.tab_style == 'vertical' %} tabbed-animation-change{% if forloop.first %} is-active{% endif %}{% endif %}"
                      x-on:mouseover="$store.xHeaderMenu.selectItem($el, true); $el.classList.add('is-focus-child');{% if block.settings.tab_style == 'vertical' %} $store.xHeaderMenu.setHeightScroll($el){% endif %}"
                      @mouseleave="$el.classList.remove('is-focus-child');{% if block.settings.tab_style == 'horizontal' %} $store.xHeaderMenu.hideMenu($el, true);{% endif %}"
                      @focusin="$store.xHeaderMenu.selectItem($el, true); $el.classList.add('is-focus-child');"
                      @focusout="$el.classList.remove('is-focus-child');"
                      {% if block.settings.tab_style == 'vertical' %}
                        x-intersect.once.margin.200px="$store.xHeaderMenu.setHeightScroll($el, {{ forloop.index }})"
                      {% endif %}
                      {%- if link.current or link.child_active %}@init-run.window="active = true;"{% endif %}
                    >
                      <div class="sub-menu {% if forloop.first %} border-t{% endif %} flex items-center z-10 relative pl-2.5 pr-2.5{% if block.settings.tab_style == 'vertical' %} w-[15.6rem]{% else %} w-[13rem]{% endif %}{% if is_rtl %} rtl{% endif %}">
                        {%- render 'menu-item-image', link: link, show_image_product: block.settings.show_image_product, show_image_collection: block.settings.show_image_collection, show_first_image_product: block.settings.show_first_image_product, class_ratio: class_image_ratio, image_left: true, collection_metafield: block.settings.collection_metafield -%}
                        <span class="flex-1">
                          <a
                            href="{{ link.url }}"
                            class="nav-link p-break-words z-10 pt-3 pb-3 flex items-center{% if link.links != blank %}{% if is_rtl %} pl-5{% else %} pr-5{% endif %}{% endif %} hover-text-link is-focus-child:effect-inline{%- if link.current or link.child_active %} flex effect-inline{% endif %}"
                            {% if link.links != blank %}@init-run.window="$store.xHeaderMenu.isTouch && $store.xHeaderMenu.touchItem($el, true)"{% endif %}
                          >
                            <span class="p-break-words">{{ link.title | escape }}</span>
                            {%- if link.links != blank -%}
                              <span class="z-10 w-5 h-4 p-1 rounded-full block absolute{% if block.settings.tab_style != 'horizontal' %}{% if is_rtl %} rotate-90 left-1.5{% else %} -rotate-90 right-1.5{% endif %}{% else %}{% if is_rtl %} left-0{% else %} right-0{% endif %}{% endif %}">
                                {% render 'icon-alls', icon: 'icon-caret' %}
                              </span>
                            {%- endif -%}
                          </a>
                        </span>
                      </div>
                      {% liquid
                        assign column = 4
                        if block.settings.promotion_layout == 'vertical'
                          if link.links.size < 4 and block.settings.tab_style == 'vertical'
                            if link.links.size > 1
                              assign column = link.links.size
                            else
                              assign column = 2
                            endif
                          endif
                          assign class_menu = 'w-full grid-cols-' | append: column
                        endif
                        if block.settings.tab_style == 'vertical'
                          assign class_menu = class_menu | remove: 'w-3/4 ' | remove: '2xl:w-3/5 ' | remove: 'w-1/2 2xl:w-5/12 ' | remove: 'w-5/12 2xl:w-[38.75%] ' | remove: '2xl:w-[62%] ' | remove: 'w-4/5 ' | append: ' w-full'
                        endif
                        assign class_menu_postion = ' left-72 rtl:right-72'

                        assign multiple_menu = false
                        for childlink in link.links
                          if childlink.links != blank
                            assign multiple_menu = true
                          endif
                        endfor
                      %}
                      <div  class="toggle-menu-sub overflow-hidden absolute right-0 rtl:left-0{% if block.settings.tab_style == 'horizontal' %} left-0 z-20 background-header{% if link.links != blank %} pt-5 pb-8{% endif %}{% else %}{{ class_menu_postion }} pt-8 z-0 top-0 pb-7 h-auto{% if forloop.first %} open{% endif %}{% endif %} toggle-menu-sub-hidden ltr">   
                        <div class="relative x-pgw page-width mx-auto h-full max-h-[calc(80vh-3rem)] overflow-y-auto scrollbar-body scrollbar-visible overflow-hidden">
                          {%- if link.links != blank -%}
                            <ul class="sub-menu-container order-2 grid gap-y-7 gap-x-7 {{ class_menu }} h-full{% if block.settings.tab_style == 'horizontal' %} pr-5 rtl:pr-0 rtl:pl-5 pb-8 overflow-y-auto scrollbar-body scrollbar-visible{% endif %}{% if is_rtl %} rtl{% endif %}">
                              {%- for childlink in link.links -%}
                                <li class="relative">
                                  <div class="{% if menu_font_weight < 600 and multiple_menu == true %}font-semibold{% endif %}">
                                    <a href="{{ childlink.url }}" class="nav-link relative transition duration-200 mb-2 p-break-words hover-text-link{% if childlink.current %} flex border-b effect-inline{% else %} inline-block{%- endif -%}">
                                      {{ childlink.title | escape }}
                                    </a>
                                    {%- render 'menu-item-image', link: childlink, show_image_product: block.settings.show_sub_image_product, show_image_collection: block.settings.show_sub_image_collection, show_first_image_product: block.settings.show_sub_first_image_product, image_ratio: block.settings.image_ratio, column: column, collection_metafield: block.settings.collection_metafield -%}
                                  </div>
                                  {%- if childlink.links != blank -%}
                                    <ul class="list-unstyled" role="list">
                                      {%- for grandchildlink in childlink.links -%}
                                        <li class="flex items-center pt-1.5 pb-1.5 relative font-normal">
                                          <span class="flex-1">
                                            <a href="{{ grandchildlink.url }}" class="nav-link relative transition duration-200 p-break-words hover-text-link{% if grandchildlink.current %} flex effect-inline border-b{% else %} inline-block{% endif %}">
                                              {{ grandchildlink.title | escape }}
                                            </a>
                                          </span>
                                        </li>
                                      {%- endfor -%}
                                    </ul>
                                  {%- endif -%}
                                </li>
                              {%- endfor -%}
                            </ul>
                          {%- endif -%}
                        </div>
                        {% if block.settings.see_more_link != blank and block.settings.see_more_title != blank %}
                          <div class="mega-sub-container relative x-pgw page-width mx-auto clear-both"
                              style="padding-top: 0{% if block.settings.tab_style == 'vertical' %}{% if is_rtl %}; padding-left: 5rem{% endif %}{% endif %}"
                            >
                            <div class="mega-sub-nav pr-5 pb-8 {{ class_menu }}">
                              <div class="see-more border-t py-4 hidden md:flex justify-{{ block.settings.see_more_alignment }} ltr">
                                <a href="{{ block.settings.see_more_link }}" 
                                  class="hover-text-link nav-link relative w-fit flex gap-1{% if is_rtl %} rtl{% endif %}"
                                  {% if block.settings.see_more_open_new_window %} target="_blank" {% endif %}
                                >
                                  {{ block.settings.see_more_title }}
                                  <span class="block w-5 h-5{% if is_rtl %} rotate-180{% endif %}">{% render 'icon-alls', icon: 'icon-arrow' %}</span>
                                </a>
                              </div>
                            </div>
                          </div>
                        {% endif %}
                      </div>
                    </li>
                  {%- endfor -%}
                </ul>
              {% else %}
                <div class="x-splide splide flex gap-x-8 disable-effect-hover visible"
                  id="horizontal-menu-{{ block.id }}"
                  x-intersect.once.margin.200px='$store.xSplide.load($el, {
                    "speed": 1000,
                    "pagination": false, 
                    "updateOnMove": "true",
                    "autoWidth": "true",
                    "gap": "2rem",
                    {%- if settings.lang_direction contains request.locale.iso_code %} 
                    "direction": "rtl",
                    {%- endif %}
                    "focus": "right",
                    "perPage": 1,
                    "perMove": 2,
                    "events": [
                      {
                        "event": "ready",
                        "callback": () => {
                          let slide = document.getElementById("horizontal-menu-{{ block.id }}");
                          let slideList = slide.querySelector(".splide__list");
                          let btnArrow = slide.querySelector(".splide__arrows"); 
                          if (slide.offsetWidth <= slideList.offsetWidth) {
                            btnArrow.classList.remove("md:hidden");
                          } else {
                            btnArrow.classList.add("md:hidden");
                          }
                        }
                      }
                    ]
                  })'
                >
                  <div class="absolute top-1/2 splide__arrows md:hidden flex justify-center w-full items-center">
                    <button class="splide__arrow splide__arrow--prev button-arrow lg:absolute top-1/2 -translate-y-1/2 rtl:right-0 rtl:-rotate-90 rtl:left-auto left-0 pl-2 pr-2 pt-2 pb-2 rounded-full none_border z-10 w-7 h-7 after:text-[20px] rotate-90 duration-200 opacity-60 hover:opacity-100 disabled:cursor-not-allowed disabled:hidden transition-all ease-in-out disabled:opacity-30" aria-label="previous slide">
                      {% render 'icon-alls', icon: 'icon-caret' %}
                    </button>
                    <button class="splide__arrow splide__arrow--next button-arrow lg:absolute top-1/2 -translate-y-1/2 right-0 rtl:left-0 rtl:rotate-90 rtl:right-auto pl-2 pr-2 pt-2 pb-2 rounded-full none_border z-10 w-7 h-7 after:text-[20px] -rotate-90 duration-200 opacity-60 hover:opacity-100 disabled:cursor-not-allowed disabled:hidden transition-all ease-in-out disabled:opacity-30" aria-label="next slide">
                      {% render 'icon-alls', icon: 'icon-caret' %}
                    </button>
                  </div>
                  <div class="splide__track">
                    <div class="splide__list flex items-end pb-0.5">
                    {% assign block_id_downcase = block.id | replace: "_", "" | downcase %}
                    {%- for link in block.settings.menu.links -%}
                      <div 
                        class="h-full sub-menu-horizontal splide__slide x-splide-slide table{% if link.links != blank %} has-dropdown{% if forloop.first %} active{% endif %}{% endif %}"
                        x-on:mouseover="$dispatch('mouseover-{{ block_id_downcase }}-{{ forloop.index }}');$el.classList.add('is-focus-child');$el.closest('.splide__list').querySelector('.sub-menu-horizontal.active')?.classList.remove('active'); $el.classList.add('active'); $dispatch('setheight-{{ block_id_downcase }}-{{ forloop.index }}');"
                        @mouseleave="$el.classList.remove('is-focus-child');"
                        @focusin="$dispatch('focusin-{{ block_id_downcase }}-{{ forloop.index }}'); $el.classList.add('is-focus-child');$el.closest('.splide__list').querySelector('.sub-menu-horizontal.active')?.classList.remove('active'); $el.classList.add('active'); $dispatch('setheight-{{ block_id_downcase }}-{{ forloop.index }}');"
                        @focusout="$el.classList.remove('is-focus-child');"
                      >
                        <div class="h-full sub-menu flex items-center z-10 relative pl-2.5 pr-2.5 w-[15.6rem]">
                          {%- render 'menu-item-image', link: link, show_image_product: block.settings.show_image_product, show_image_collection: block.settings.show_image_collection, show_first_image_product: block.settings.show_first_image_product, class_ratio: class_image_ratio, image_left: true, collection_metafield: block.settings.collection_metafield -%}
                          <span class="flex-1">
                              <a href="{{ link.url }}"
                                class="nav-link w-full text-left p-break-words z-10 pb-3 pt-3 flex items-center{% if link.links != blank %}{% if is_rtl %} pl-5{% else %} pr-5{% endif %}{% endif %} hover-text-link is-focus-child:effect-inline{%- if link.current or link.child_active %} flex effect-inline{% endif %}"
                                {% if link.links != blank %}@init-run.window="$store.xHeaderMenu.isTouch && $store.xHeaderMenu.touchItem($el, true)"{% endif %}
                              >
                              <span class="p-break-words">{{ link.title | escape }}</span>
                              {%- if link.links != blank -%}
                                <span class="z-10 w-5 h-4 p-1 rounded-full block absolute{% if block.settings.tab_style != 'horizontal' %}{% if is_rtl %} rotate-90 left-1.5{% else %} -rotate-90 right-1.5{% endif %}{% else %}{% if is_rtl %} left-0{% else %} right-0{% endif %}{% endif %}">
                                  {% render 'icon-alls', icon: 'icon-caret' %}
                                </span>
                              {%- endif -%}
                              </a>
                          </span>
                        </div>
                      </div>
                    {% endfor %}
                    </div>
                  </div>
                </div>
                <ul class="mega-list-nav items-end{% if is_rtl %} rtl justify-start{% endif %} h-0 pt-0 pb-0 overflow-hidden" x-refs="mega_nav"> 
                  {%- for link in block.settings.menu.links -%}
                    <li
                      class="sub-menu-item table{% if link.links != blank %} has-dropdown{% endif %}{% if block.settings.tab_style == 'vertical' %} tabbed-animation-change{% if forloop.first %} is-active{% endif %}{% endif %}"
                      {% if link.links != blank and forloop.first %}
                        x-intersect.once.margin.200px="$store.xHeaderMenu.selectItem($el, true); $el.classList.add('is-focus-child');$store.xHeaderMenu.tongleHorizontalHeight($el);"
                      {% endif %}
                        @setheight-{{ block_id_downcase }}-{{ forloop.index }}.window="$store.xHeaderMenu.tongleHorizontalHeight($el);"
                        @mouseover-{{ block_id_downcase }}-{{ forloop.index }}.window="$store.xHeaderMenu.selectItem($el, true); $el.classList.add('is-focus-child');"
                        @mouseleave="$el.classList.remove('is-focus-child');"
                        @focusin-{{ block_id_downcase }}-{{ forloop.index }}.window="$store.xHeaderMenu.selectItem($el, true); $el.classList.add('is-focus-child');"
                        @focusout="$el.classList.remove('is-focus-child');"
                      {% if block.settings.tab_style == 'vertical' %}
                        x-intersect.once.margin.200px="$store.xHeaderMenu.setHeightScroll($el, {{ forloop.index }})"
                      {% endif %}
                      {%- if link.current or link.child_active %}@init-run.window="active = true;"{% endif %}
                    >
                      <div class="h-0 sub-menu {% if forloop.first %} border-t{% endif %} flex items-center z-10 relative pl-2.5 pr-2.5{% if block.settings.tab_style == 'vertical' %} w-[15.6rem]{% else %} w-[13rem]{% endif %}{% if is_rtl %} rtl{% endif %}">
                        {%- render 'menu-item-image', link: link, show_image_product: block.settings.show_image_product, show_image_collection: block.settings.show_image_collection, show_first_image_product: block.settings.show_first_image_product, class_ratio: class_image_ratio, image_left: true, collection_metafield: block.settings.collection_metafield -%}
                        <span class="flex-1">
                          <a
                            href="{{ link.url }}"
                            class="nav-link p-break-words z-10 pt-3 pb-3 flex items-center{% if link.links != blank %}{% if is_rtl %} pl-5{% else %} pr-5{% endif %}{% endif %} hover-text-link is-focus-child:effect-inline{%- if link.current or link.child_active %} flex effect-inline{% endif %}"
                            {% if link.links != blank %}@init-run.window="$store.xHeaderMenu.isTouch && $store.xHeaderMenu.touchItem($el, true)"{% endif %}
                          >
                            <span class="p-break-words">{{ link.title | escape }}</span>
                            {%- if link.links != blank -%}
                              <span class="z-10 w-5 h-4 p-1 rounded-full block absolute{% if block.settings.tab_style != 'horizontal' %}{% if is_rtl %} rotate-90 left-1.5{% else %} -rotate-90 right-1.5{% endif %}{% else %}{% if is_rtl %} left-0{% else %} right-0{% endif %}{% endif %}">
                                {% render 'icon-alls', icon: 'icon-caret' %}
                              </span>
                            {%- endif -%}
                          </a>
                        </span>
                      </div>
                      {% liquid
                        assign column = 4
                        if block.settings.promotion_layout == 'vertical'
                          if link.links.size < 4 and block.settings.tab_style == 'vertical'
                            if link.links.size > 1
                              assign column = link.links.size
                            else
                              assign column = 2
                            endif
                          endif
                          assign class_menu = 'w-full grid-cols-' | append: column
                        endif
                        if block.settings.tab_style == 'vertical'
                          assign class_menu = class_menu | remove: 'w-3/4 ' | remove: '2xl:w-3/5 ' | remove: 'w-1/2 2xl:w-5/12 ' | remove: 'w-5/12 2xl:w-[38.75%] ' | remove: '2xl:w-[62%] ' | remove: 'w-4/5 ' | append: ' w-full'
                        endif
                        assign class_menu_postion = ' left-72 rtl:right-72'

                        assign multiple_menu = false
                        for childlink in link.links
                          if childlink.links != blank
                            assign multiple_menu = true
                          endif
                        endfor
                      %}
                      <div class="toggle-menu-sub overflow-hidden absolute right-0 rtl:left-0 overflow-y-auto scrollbar-body scrollbar-visible{% if block.settings.tab_style == 'horizontal' %} left-0 z-20 background-header{% if link.links != blank %} pt-10 pb-8{% endif %}{% else %}{{ class_menu_postion }} pt-12 z-0 top-0 pb-7 h-auto max-h-[calc(80vh-3rem)]{% endif %} toggle-menu-sub-hidden ltr">   
                        <div class="relative x-pgw page-width mx-auto h-full">
                          {%- if link.links != blank -%}
                            <ul class="sub-menu-container order-2 grid gap-y-7 gap-x-7 w-full h-full{% if block.settings.tab_style == 'horizontal' %} {{ class_menu_horizontal }}{% if block.settings.promotion_position == 'left' %} float-right{% endif %} pb-8 overflow-y-auto scrollbar-body scrollbar-visible{% else %} {{ class_menu }}{% endif %}{% if is_rtl %} rtl{% endif %}">
                              {%- for childlink in link.links -%}
                                <li class="relative">
                                  <div class="{% if menu_font_weight < 600 and multiple_menu == true %}font-semibold{% endif %}">
                                    <a href="{{ childlink.url }}" class="nav-link relative transition duration-200 mb-2 p-break-words hover-text-link{% if childlink.current %} flex border-b effect-inline{% else %} inline-block{%- endif -%}">
                                      {{ childlink.title | escape }}
                                    </a>
                                    {%- render 'menu-item-image', link: childlink, show_image_product: block.settings.show_sub_image_product, show_image_collection: block.settings.show_sub_image_collection, show_first_image_product: block.settings.show_sub_first_image_product, image_ratio: block.settings.image_ratio, column: column, collection_metafield: block.settings.collection_metafield -%}
                                  </div>
                                  {%- if childlink.links != blank -%}
                                    <ul class="list-unstyled" role="list">
                                      {%- for grandchildlink in childlink.links -%}
                                        <li class="flex items-center pt-1.5 pb-1.5 relative font-normal">
                                          <span class="flex-1">
                                            <a href="{{ grandchildlink.url }}" class="nav-link relative transition duration-200 p-break-words hover-text-link{% if grandchildlink.current %} flex effect-inline border-b{% else %} inline-block{% endif %}">
                                              {{ grandchildlink.title | escape }}
                                            </a>
                                          </span>
                                        </li>
                                      {%- endfor -%}
                                    </ul>
                                  {%- endif -%}
                                </li>
                              {%- endfor -%}
                            </ul>
                          {%- endif -%}
                        </div>
                        {% if block.settings.see_more_link != blank and block.settings.see_more_title != blank %}
                          <div class="mega-sub-container relative x-pgw page-width mx-auto clear-both{% if block.settings.tab_style == 'vertical' %}{% if is_rtl %} pr-[20rem]{% else %} pl-[20rem]{% endif %}{% endif %}"
                              style="padding-top: 0{% if block.settings.tab_style == 'vertical' %}{% if is_rtl %}; padding-left: 5rem{% endif %}{% endif %}"
                            >
                            <div class="mega-sub-nav pr-5 pb-8 {{ class_menu }}">
                              <div class="see-more border-t py-4 hidden md:flex justify-{{ block.settings.see_more_alignment }} ltr">
                                <a href="{{ block.settings.see_more_link }}" 
                                  class="hover-text-link nav-link relative w-fit flex gap-1{% if is_rtl %} rtl{% endif %}"
                                  {% if block.settings.see_more_open_new_window %} target="_blank" {% endif %}
                                >
                                  {{ block.settings.see_more_title }}
                                  <span class="block w-5 h-5{% if is_rtl %} rotate-180{% endif %}">{% render 'icon-alls', icon: 'icon-arrow' %}</span>
                                </a>
                              </div>
                            </div>
                          </div>
                        {% endif %}
                      </div>
                    </li>
                  {%- endfor -%}
                </ul>
              {% endif %}
            </div>
            {%- if block.settings.enable_quick_access_blocks -%}
              {% assign block_id = block.id | append: "-quick-access" %}
              {%- style -%}
                .button-{{ block_id }} .icon-access  {
                  width: {{ block.settings.quick_access_icon_size }}px;
                  height: {{ block.settings.quick_access_icon_size }}px;
                  min-width: {{ block.settings.quick_access_icon_size }}px;
                }
